$0 = 'pry'

$LOAD_PATH.unshift('.')
require 'config/environment'

require 'pry'
require 'pryx'
require 'pryx/ap_hack'

# Process command line options and run Pry
opts = Pry::CLI.parse_options
Pry::CLI.start(opts)
